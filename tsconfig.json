{
    "compilerOptions": {
        "module": "CommonJS",
        "esModuleInterop": true,
        "strict": true,
        "resolveJsonModule": true,
        "target": "ES2018",
        "types": [
            "node",
        ],
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "/",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}